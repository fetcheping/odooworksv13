<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for sale.order -->
        <record id="seq_examen" model="ir.sequence">
            <field name="name">Examen</field>
            <field name="code">examen.sequence</field>
            <field name="prefix">EX</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_patient" model="ir.sequence">
            <field name="name">patient</field>
            <field name="code">patient.sequence</field>
            <field name="prefix">PA</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>


    </data>
</odoo>
